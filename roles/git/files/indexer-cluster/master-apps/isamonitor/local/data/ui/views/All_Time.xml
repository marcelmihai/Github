<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>All Time</label>
  <row>
    <chart>
      <searchName>FWS Top Clients By Request</searchName>
      <title>Firewall Client Activity</title>
      <option name="charting.axisTitleX.text">Client IP</option>
      <option name="charting.axisTitleY.text">Requests</option>
      <option name="charting.axisY.scale">log</option>
      <option name="charting.chart">bar</option>
      <option name="charting.legend.placement">none</option>
      <option name="charting.primaryAxisTitle.text"/>
      <option name="charting.secondaryAxis.scale">log</option>
      <option name="charting.secondaryAxisTitle.text"/>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </chart>
    <chart>
      <searchName>FWS Top Destinations</searchName>
